set BronzeChestB=No
set BronzeChestE=No
set IronChestA=130
set IronChestB=No
set IronChestE=No
set DragonChest=175
set DragonChestB=No
set DragonChestE=No
set EnergyChestA=220
set EnergyChestB=No
set EnergyChestE=No
set HideLegsA=30
set HideLegsB=No
set HideLegsE=No
set BronzeLegsA=55
set BronzeLegsB=No
set BronzeLegsE=No
set IronLegsA=90
set IronLegsB=No
set IronLegsE=No
set DragonLegsA=125
set DragonLegsB=No
set DragonLegsE=No
set EnergyLegsA=160
set EnergyLegsB=No
set EnergyLegsE=No
set HideBootsA=15
set HideBootsB=No
set HideBootsE=No
