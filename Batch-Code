echo zombiesKilled=%zombiesKilled%
echo ghostsKilled=%ghostsKilled%
echo WeaponE=%WeaponE%
echo WeaponD=%WeaponD%
echo FistKills=%FistKills%
echo DaggerD=30
echo DaggerB=%DaggerB%
echo DaggerE=%DaggerE%
