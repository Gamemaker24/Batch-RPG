set BattleaxeB=No
set BattleaxeE=No
set BattleaxeKills=0
set StrengthP=0
set HealthP=0
set SpeedP=0
set ManaP=0
set StrengthPUse=No
set SpeedPUse=No
set PureDSpellB=No
set FreezeSpellB=No
set FireSpellB=No
set PureDSpellUse=No
set FreezeSpellUse=No
set FireSpellUse=No
set FreezeroundsLeft=0
set FireRoundsLeft=0
set PureDSpellTier=0
set FreezeSpellTier=0
set FireSpellTier=0
set Armor=0
set HelmA=0
set HelmE=None
set ChestA=0
set ChestE=None
set LegsA=0
set LegsE=None
set BootsA=0
set BootsE=None
set HideHelmA=20
set HideHelmB=No
set HideHelmE=No
set BronzeHelmA=45
set BronzeHelmB=No
set BronzeHelmE=No
set IronHelmA=75
set IronHelmB=No
set IronHelmE=No
set DragonHelmA=95
set DragonHelmB=No
set DragonHelmE=No
set EnergyHelmA=125
set EnergyHelmB=No
set EnergyHelmE=No
